{
    "compilerOptions": {
        "target": "ES2016",
        "module": "Node16",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "Node16",
        "sourceMap": true,
        "outDir": "build",
        "types": [
            "jest"
        ]
    },
    "include": [
        "src/**/*.ts",
        "test/**/*.ts"
    ],
    "exclude": [
        "node_modules/",
    ]
}